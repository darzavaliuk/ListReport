	<MessagePopover items="{messages>/}" initiallyExpanded="true" activeTitlePress="onPressItemMessageItem">
		<items>
			<MessagePopoverItem type="{messages>type}" title="{messages>message}" activeTitle="true" />
		</items>
	</MessagePopover>
</FragmentDefinition>
