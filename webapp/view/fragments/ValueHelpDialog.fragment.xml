<FragmentDefinition xmlns="sap.m" xmlns:vh="sap.ui.comp.valuehelpdialog" xmlns:fb="sap.ui.comp.filterbar" xmlns:core="sap.ui.core">
	<ValueHelpDialog title="{i18n>labelFilterBarProduct}" ok="onDialogOkPress" cancel="onValueHelpDialogCancelPress" afterClose="onWhitespaceAfterClose" supportRanges="true" key="Price" descriptionKey="ProductName">
		<filterBar>
			<FilterBar advancedMode="true" search="onFilterBarSearch" isRunningInValueHelpDialog="true">
				<filterGroupItems>
					<FilterGroupItem groupName="Basic" name="Price" label="{i18n>labelFilterBarPrice}" visibleInFilterBar="true">
						<control>
							<Input name="Price" showSuggestion="true" showValueHelp="false" suggestionItems="{ path: '/Products', sorter: { path: 'Price' } }">
								<Item key="{Price}" text="{ path: 'Price' }" />
							</Input>
						</control>
					</FilterGroupItem>
					<FilterGroupItem groupName="Basic" name="Name" label="{i18n>filterBarNameLabel}" visibleInFilterBar="true">
						<control>
							<Input name="Name" showSuggestion="true" showValueHelp="false" suggestionItems="{ path: '/Products', sorter: { path: 'Name' } }">
								<Item key="{Name}" text="{Name}" />
							</Input>
						</control>
					</FilterGroupItem>
				</filterGroupItems>
			</FilterBar>
		</filterBar>
	</ValueHelpDialog>
</FragmentDefinition>
