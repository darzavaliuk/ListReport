<core:FragmentDefinition
        xmlns="sap.m"
        xmlns:core="sap.ui.core">
    <SelectDialog id="selectDialog" title="Products" items="{/Products}" search="onValueHelpSearch"
                  confirm="onValueHelpClose" cancel="onValueHelpClose">
        <StandardListItem iconDensityAware="false" iconInset="false" title="{Name}"
                          description="{Description}"/>
    </SelectDialog>
</core:FragmentDefinition>
